{
  "version": 2,
  "name": "backend",
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST"], // Specify the HTTP methods you want to increase the timeout for
      "headers": {
        "x-vercel-idle-timeout": "600" // Set the timeout value in seconds
      }
    }
  ]
}
